<Plugin "python">
  ModulePath "/opt/collectd/share/collectd/"

  Import "collectd-elasticsearch"

  <Module "collectd-elasticsearch">
    Url "<%= @protocol %>://<%= @host %>:<%= @port %>"
    <% if !@cluster_name.empty? %>
    Name "<%= @cluser_name %>"
    <% end %>
    Verbose <%= @verbose %>
  </Module>
</Plugin>
