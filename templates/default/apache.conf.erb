LoadPlugin "apache"
<Plugin "apache">
   <Instance "apache80">
       URL "<%= @protocol %>://<%= @host %><%= @path %>?auto"
       <% if !@user.nil? -%>
       User "<%= @user %>"
       <% end -%>
       <% if !@password.nil? -%>
       Password "<%= @password %>"
       <% end -%>
   </Instance>
</Plugin>
